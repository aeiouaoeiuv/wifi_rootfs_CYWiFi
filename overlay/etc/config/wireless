
config wifi-device 'mt7620'
	option type 'mt7620'
	option vendor 'ralink'
	option band '2.4G'
	option channel 'auto'
	option txpoer '100'
	option auotch '2'
	option enable '1'

config wifi-iface
	option device 'mt7620'
	option ifname 'ra0'
	option network 'lan'
	option mode 'ap'
	option ssid 'Knowifi_AI'
	option encryption 'psk-mixed+tkip+ccmp'
	option key 'smartecs123654'

config wifi-iface
	option device 'mt7620'
	option ifname 'ra1'
	option network 'lan'
	option mode 'ap'
	option encryption 'none'
	option enable '0'
	option ssid 'Knowifi_AI-Guest'

config wifi-time
	option time_on '0'
	option loop '0'
	option day '0'
	option starth '0'
	option startm '0'
	option endh '0'
	option endm '0'

